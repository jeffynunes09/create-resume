{
	"name": "create-resume",
	"version": "0.0.0",
	"private": true,
	"allowImportingTsExtensions": true,
	"type": "module",
	"scripts": {
		"dev": "turbo run dev",
		"build": "turbo run build",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"typecheck": "biome check .",
		"test": "turbo run test",
		"clean": "turbo run clean && rm -rf node_modules",
		"format": "biome format --write .",
		"storybook": "pnpm --filter @create-resume/web storybook",
		"build-storybook": "pnpm --filter @create-resume/web build-storybook"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.13",
		"prettier": "^3.4.0",
		"turbo": "^2.4.0",
		"typescript": "~5.9.3"
	},
	"packageManager": "pnpm@9.15.0",
	"engines": {
		"node": ">=20"
	}
}
